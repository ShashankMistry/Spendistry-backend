const mongoose = require('mongoose');

const ItemPricesSchema = new mongoose.Schema({
    _id : {
        type: String
    },
    ItemsPrices: {
        type: [{
            _id: {
                type: String
            },
            barcode : String,
            itemName : String,
            price : String
        }]
    }
});

module.exports = mongoose.model('ItemPricesSchema', ItemPricesSchema);